{
    "dir": "ltr",
    "lang": "en",
    "name": "PWA Share Target",
    "scope": ".",
    "display": "standalone",
    "start_url": "./",
    "short_name": "PWA Share Target",
    "theme_color": "#01579B",
    "description": "A test of the Web Share Target API for PWAs",
    "orientation": "Portrait",
    "background_color": "#212121",
    "related_applications": [],
    "prefer_related_applications": false,
    "icons": [
        {
            "src": "images/icons/icon-192x192.png",
            "sizes": "192x192",
            "purpose": "any maskable"
        },
        {
            "src": "images/icons/icon-512x512.png",
            "sizes": "512x512",
            "purpose": "any maskable"
        }
    ],
    "share_target": {
        "action": "?share-target",
        "method": "POST",
        "enctype": "multipart/form-data",
        "params": {
            "files": [
                {
                    "name": "image",
                    "accept": "image/*"
                }
            ]
        }
    }
}
